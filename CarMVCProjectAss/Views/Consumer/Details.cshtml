@model CarMVCProjectAss.Models.Consumer
@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>
<table class="table table-bordered table-striped table-hover">
    <tr>
        <th>FirstName</th>
        <th>LastName</th>
        <th>Email</th>
        <th>City</th>
        <th>Address</th>
        <th>PhoneNumber</th>
    </tr>

    <tr>
    <td>@Model.FirstName</td>
    <td>@Model.LastName</td>
    <td>@Model.Email</td>
    <td>@Model.City</td>
    <td>@Model.Address</td>
    <td>@Model.PhoneNumber</td>
    </tr>

</table>